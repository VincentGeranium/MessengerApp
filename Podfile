platform :ios, '13.0'

target 'MessengerApp' do
  use_frameworks!

# Firebase

	pod 'Firebase/Core'
	pod 'Firebase/Storage'
	pod 'Firebase/Database'
	pod 'Firebase/Auth'
	pod 'Firebase/Analytics'
	pod 'Firebase/Crashlytics'

# Facebook

	pod 'FBSDKCoreKit'
	pod 'FBSDKLoginKit'
	pod 'FBSDKShareKit'

# Google
	
	pod 'GoogleSignIn'

end
